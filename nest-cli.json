{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "appointment": {
      "type": "application",
      "root": "apps/appointment",
      "entryFile": "main",
      "sourceRoot": "apps/appointment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/appointment/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "billing": {
      "type": "application",
      "root": "apps/billing",
      "entryFile": "main",
      "sourceRoot": "apps/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing/tsconfig.app.json"
      }
    },
    "common-utils": {
      "type": "library",
      "root": "libs/common-utils",
      "entryFile": "index",
      "sourceRoot": "libs/common-utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-utils/tsconfig.lib.json"
      }
    },
    "doctor": {
      "type": "application",
      "root": "apps/doctor",
      "entryFile": "main",
      "sourceRoot": "apps/doctor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/doctor/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "patient": {
      "type": "application",
      "root": "apps/patient",
      "entryFile": "main",
      "sourceRoot": "apps/patient/src",
      "compilerOptions": {
        "tsConfigPath": "apps/patient/tsconfig.app.json"
      }
    }
  }
}